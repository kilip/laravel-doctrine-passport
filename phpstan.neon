parameters:
    paths:
        - src
    level: 5
    checkMissingIterableValueType: false
    excludePaths:
        - src/*/*/Database/Factories/*
        - src/*/*/Tests/*
    ignoreErrors:
        - '#Call to an undefined method [a-zA-Z0-9\\_]+::giveConfig#'
        - '#undefined property [a-zA-Z0-9\\_]+::\$name#'
        - '#undefined property [a-zA-Z0-9\\_]+::\$redirect#'